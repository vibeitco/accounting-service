    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "env": {
                "SERVICE": "accounting",
                "ENV": "local",
                "VERSION": "1",
                "DOMAIN": "localhost:8080",
                "DOMAIN_API": "localhost:8080",
                "DOMAIN_ASSETS": "https://assets",
        
            },
            "program": "${workspaceFolder}"
        },
        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "port": 8080,
            "host": "127.0.0.1",
            "apiVersion": 1,
            "showLog": true
        }
    ]
}